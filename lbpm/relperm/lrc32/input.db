   OutletLayers = 0, 0, 5 
   Sw = 0.3
}
Color {
   ComponentLabels = 0, 
   ComponentAffinity = 0.9, 
   WettingConvention = "SCAL"
   protocol = "fractional flow"
   rhoA = 1.0
   rhoB = 1.0
   tauA = 0.7
   tauB = 0.7
   alpha = 0.008
   beta = 0.95
   F = 0.0, 0.0, 1e-05
   capillary_number = 1e-04
