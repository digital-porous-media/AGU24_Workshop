Domain {
   Filename = "segmented_lrc32_512.ubc.morphdrain.raw"
   voxel_length = 9.184
   N = 256, 256, 256
   n = 256, 256, 256
   nproc = 1, 1, 1
   ReadType ="8bit"
   // key values set by image labeling 
   ReadValues = 0, 1, 2 
   WriteValues = 0, 1, 2 
   // keys below set by color model
   BC = 0
